<?xml version="1.0" encoding="UTF-8"?>
<LinearLayout
    xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    android:layout_width="wrap_content"
    android:layout_height="wrap_content">

    <LinearLayout
        android:id="@+id/linearColumnarrowleft"
        style="@style/groupStylewhite_A700"
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:layout_marginStart="@dimen/_20pxh"
        android:gravity="start"
        android:orientation="vertical">

        <androidx.appcompat.widget.Toolbar
            android:id="@+id/toolbarToolbar"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:layout_marginEnd="@dimen/_10pxh"
            app:contentInsetStart="0dp">

            <LinearLayout
                android:id="@+id/linearHead"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:orientation="horizontal">

                <ImageView
                    android:id="@+id/imageArrowleft"
                    android:layout_width="@dimen/_17pxh"
                    android:layout_height="@dimen/_17pxh"
                    android:layout_marginTop="@dimen/_5pxv"
                    android:layout_marginBottom="@dimen/_5pxv"
                    android:scaleType="fitXY"
                    android:src="@drawable/img_arrowleft"/>

                <LinearLayout
                    android:id="@+id/linearRowfilters"
                    android:layout_width="0dp"
                    android:layout_height="wrap_content"
                    android:layout_marginStart="@dimen/_123pxh"
                    android:layout_weight="0.92"
                    android:orientation="horizontal">

                    <TextView
                        android:id="@+id/txtFilters"
                        style="@style/txtPoppinssemibold18"
                        android:layout_width="0dp"
                        android:layout_height="wrap_content"
                        android:layout_marginBottom="@dimen/_1pxv"
                        android:layout_weight="0.48"
                        android:ellipsize="end"
                        android:singleLine="true"
                        android:text="@string/lbl_filters" />

                    <TextView
                        android:id="@+id/txtApply"
                        style="@style/txtPoppinssemibold18_1"
                        android:layout_width="0dp"
                        android:layout_height="wrap_content"
                        android:layout_marginStart="@dimen/_77pxh"
                        android:layout_marginTop="@dimen/_1pxv"
                        android:layout_weight="0.52"
                        android:ellipsize="end"
                        android:letterSpacing="0.04"
                        android:singleLine="true"
                        android:text="@string/lbl_apply"
                        android:textAllCaps="true" />
                </LinearLayout>
            </LinearLayout>
        </androidx.appcompat.widget.Toolbar>

        <LinearLayout
            android:id="@+id/linear6Filter"
            style="@style/groupStylewhite_A700"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:layout_marginTop="@dimen/_37pxv"
            android:gravity="start"
            android:orientation="vertical">

            <LinearLayout
                android:id="@+id/linearColumnconnectionavai"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:layout_marginEnd="@dimen/_10pxh"
                android:gravity="start"
                android:orientation="vertical">

                <TextView
                    android:id="@+id/txtConnectionAvai"
                    style="@style/txtPoppinsmedium16"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:layout_marginEnd="@dimen/_10pxh"
                    android:ellipsize="end"
                    android:singleLine="true"
                    android:text="@string/msg_connection_avai" />

                <androidx.recyclerview.widget.RecyclerView
                    android:id="@+id/recyclerGridjcounter"
                    itemSpace="@{@dimen/_15pxh}"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:layout_marginTop="@dimen/_20pxv"
                    app:layoutManager="androidx.recyclerview.widget.GridLayoutManager"
                    app:spanCount="3"/>
            </LinearLayout>

            <LinearLayout
                android:id="@+id/linearColumnspeeds"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:layout_marginTop="@dimen/_33pxv"
                android:gravity="start"
                android:orientation="vertical">

                <TextView
                    android:id="@+id/txtSpeeds"
                    style="@style/txtPoppinsmedium16"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_marginEnd="@dimen/_10pxh"
                    android:ellipsize="end"
                    android:singleLine="true"
                    android:text="@string/lbl_speeds"/>

                <LinearLayout
                    android:id="@+id/linearRowcontrast"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:layout_marginTop="@dimen/_16pxv"
                    android:gravity="start"
                    android:orientation="horizontal">

                    <ImageView
                        android:id="@+id/imageContrast"
                        android:layout_width="@dimen/_16pxh"
                        android:layout_height="@dimen/_16pxh"
                        android:layout_marginTop="@dimen/_2pxv"
                        android:layout_marginBottom="@dimen/_4pxv"
                        android:scaleType="fitXY"
                        android:src="@drawable/img_contrast_bluegray_500"/>

                    <TextView
                        android:id="@+id/txtStandardThirtySeven"
                        style="@style/txtPoppinsmedium16"
                        android:layout_width="0dp"
                        android:layout_height="wrap_content"
                        android:layout_marginStart="@dimen/_22pxh"
                        android:layout_weight="0.95"
                        android:ellipsize="end"
                        android:lineSpacingExtra="@dimen/_8pxh"
                        android:singleLine="true"
                        android:text="@string/msg_standard_3_7" />
                </LinearLayout>

                <ImageView
                    android:id="@+id/imageCheckmark"
                    android:layout_width="@dimen/_16pxh"
                    android:layout_height="@dimen/_16pxh"
                    android:layout_marginTop="@dimen/_23pxv"
                    android:layout_marginEnd="@dimen/_10pxh"
                    android:scaleType="fitXY"
                    android:src="@drawable/img_checkmark"/>

                <ImageView
                    android:id="@+id/imageContrastOne"
                    android:layout_width="@dimen/_16pxh"
                    android:layout_height="@dimen/_16pxh"
                    android:layout_marginStart="@dimen/_1pxh"
                    android:layout_marginTop="@dimen/_27pxv"
                    android:layout_marginEnd="@dimen/_10pxh"
                    android:scaleType="fitXY"
                    android:src="@drawable/img_contrast_bluegray_500" />

                <ImageView
                    android:id="@+id/imageContrastTwo"
                    android:layout_width="@dimen/_16pxh"
                    android:layout_height="@dimen/_16pxh"
                    android:layout_marginStart="@dimen/_1pxh"
                    android:layout_marginTop="@dimen/_28pxv"
                    android:layout_marginEnd="@dimen/_10pxh"
                    android:scaleType="fitXY"
                    android:src="@drawable/img_contrast_bluegray_500"/>
            </LinearLayout>

            <LinearLayout
                android:id="@+id/linearColumnamenities"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:layout_marginTop="@dimen/_35pxv"
                android:layout_marginEnd="@dimen/_10pxh"
                android:gravity="start"
                android:orientation="vertical">

                <TextView
                    android:id="@+id/txtAmenities"
                    style="@style/txtPoppinsmedium16"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_marginEnd="@dimen/_10pxh"
                    android:ellipsize="end"
                    android:singleLine="true"
                    android:text="@string/lbl_amenities"/>

                <LinearLayout
                    android:id="@+id/linearRowicpharmacy"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:layout_marginStart="@dimen/_13pxh"
                    android:layout_marginTop="@dimen/_9pxv"
                    android:layout_marginEnd="@dimen/_1pxh"
                    android:orientation="horizontal">

                    <LinearLayout
                        android:id="@+id/linearRowicwashroom"
                        android:layout_width="0dp"
                        android:layout_height="wrap_content"
                        android:layout_weight="0.81"
                        android:orientation="horizontal">

                        <ImageView
                            android:id="@+id/imageIcwashroom"
                            android:layout_width="@dimen/_41pxh"
                            android:layout_height="@dimen/_39pxh"
                            android:layout_marginTop="@dimen/_9pxv"
                            android:scaleType="fitXY"
                            android:src="@drawable/img_icwashroom_15x15"/>

                        <ImageView
                            android:id="@+id/imageCheckmarkOne"
                            android:layout_width="@dimen/_16pxh"
                            android:layout_height="@dimen/_16pxh"
                            android:layout_marginStart="@dimen/_3pxh"
                            android:layout_marginBottom="@dimen/_32pxv"
                            android:scaleType="fitXY"
                            android:src="@drawable/img_checkmark"/>

                        <ImageView
                            android:id="@+id/imageIcfood"
                            android:layout_width="@dimen/_43pxh"
                            android:layout_height="@dimen/_41pxh"
                            android:layout_marginStart="@dimen/_26pxh"
                            android:layout_marginTop="@dimen/_3pxv"
                            android:layout_marginBottom="@dimen/_4pxv"
                            android:scaleType="fitXY"
                            android:src="@drawable/img_icfood_15x16"/>

                        <ImageView
                            android:id="@+id/imageIcshopping"
                            android:layout_width="@dimen/_43pxh"
                            android:layout_height="@dimen/_40pxh"
                            android:layout_marginStart="@dimen/_45pxh"
                            android:layout_marginTop="@dimen/_6pxv"
                            android:layout_marginBottom="@dimen/_2pxv"
                            android:scaleType="fitXY"
                            android:src="@drawable/img_icshopping_15x16"/>
                    </LinearLayout>

                    <FrameLayout
                        android:id="@+id/frameStackicpharmacy"
                        android:layout_width="@dimen/_50pxh"
                        android:layout_height="@dimen/_46pxv"
                        android:layout_marginStart="@dimen/_54pxh"
                        android:layout_marginBottom="@dimen/_2pxv">

                        <ImageView
                            android:id="@+id/imageIcpharmacy"
                            android:layout_width="@dimen/_39pxh"
                            android:layout_height="@dimen/_40pxh"
                            android:layout_gravity="bottom|start"
                            android:layout_marginTop="@dimen/_10pxv"
                            android:layout_marginEnd="@dimen/_10pxh"
                            android:scaleType="fitXY"
                            android:src="@drawable/img_icpharmacy_15x15"/>

                        <ImageView
                            android:id="@+id/imageCheckmarkTwo"
                            android:layout_width="@dimen/_16pxh"
                            android:layout_height="@dimen/_16pxh"
                            android:layout_gravity="top|end"
                            android:layout_marginStart="@dimen/_10pxh"
                            android:layout_marginBottom="@dimen/_10pxv"
                            android:scaleType="fitXY"
                            android:src="@drawable/img_checkmark"/>
                    </FrameLayout>
                </LinearLayout>

                <LinearLayout
                    android:id="@+id/linearRowwashroom"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:layout_marginTop="@dimen/_10pxv"
                    android:orientation="horizontal">

                    <TextView
                        android:id="@+id/txtWashroom"
                        style="@style/txtPoppinsmedium12"
                        android:layout_width="0dp"
                        android:layout_height="wrap_content"
                        android:layout_marginBottom="@dimen/_1pxv"
                        android:layout_weight="0.3"
                        android:ellipsize="end"
                        android:singleLine="true"
                        android:text="@string/lbl_washroom" />

                    <TextView
                        android:id="@+id/txtFoods"
                        style="@style/txtPoppinsmedium12"
                        android:layout_width="0dp"
                        android:layout_height="wrap_content"
                        android:layout_marginStart="@dimen/_37pxh"
                        android:layout_marginBottom="@dimen/_1pxv"
                        android:layout_weight="0.16"
                        android:ellipsize="end"
                        android:singleLine="true"
                        android:text="@string/lbl_foods" />

                    <TextView
                        android:id="@+id/txtShopping"
                        style="@style/txtPoppinsmedium12"
                        android:layout_width="0dp"
                        android:layout_height="wrap_content"
                        android:layout_marginStart="@dimen/_39pxh"
                        android:layout_marginTop="@dimen/_1pxv"
                        android:layout_marginBottom="@dimen/_1pxv"
                        android:layout_weight="0.26"
                        android:ellipsize="end"
                        android:singleLine="true"
                        android:text="@string/lbl_shopping" />

                    <TextView
                        android:id="@+id/txtPharmacy"
                        style="@style/txtPoppinsmedium12"
                        android:layout_width="0dp"
                        android:layout_height="wrap_content"
                        android:layout_marginStart="@dimen/_35pxh"
                        android:layout_marginTop="@dimen/_1pxv"
                        android:layout_weight="0.28"
                        android:ellipsize="end"
                        android:singleLine="true"
                        android:text="@string/lbl_pharmacy"/>
                </LinearLayout>

                <LinearLayout
                    android:id="@+id/linearRowicpharmacyOne"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:layout_marginStart="@dimen/_13pxh"
                    android:layout_marginTop="@dimen/_12pxv"
                    android:layout_marginEnd="@dimen/_13pxh"
                    android:orientation="horizontal">

                    <ImageView
                        android:id="@+id/imageIcpharmacyOne"
                        android:layout_width="@dimen/_39pxh"
                        android:layout_height="@dimen/_40pxh"
                        android:layout_marginTop="@dimen/_13pxv"
                        android:layout_marginBottom="@dimen/_2pxv"
                        android:scaleType="fitXY"
                        android:src="@drawable/img_icpharmacy_15x15" />

                    <FrameLayout
                        android:id="@+id/frameStackicwifi"
                        android:layout_width="@dimen/_56pxh"
                        android:layout_height="@dimen/_52pxv"
                        android:layout_marginStart="@dimen/_42pxh"
                        android:layout_marginBottom="@dimen/_3pxv">

                        <ImageView
                            android:id="@+id/imageIcwifi"
                            android:layout_width="@dimen/_51pxh"
                            android:layout_height="@dimen/_39pxh"
                            android:layout_gravity="bottom|start"
                            android:layout_marginTop="@dimen/_10pxv"
                            android:layout_marginEnd="@dimen/_10pxh"
                            android:scaleType="fitXY"
                            android:src="@drawable/img_icwifi_39x51"/>

                        <ImageView
                            android:id="@+id/imageCheckmarkThree"
                            android:layout_width="@dimen/_16pxh"
                            android:layout_height="@dimen/_16pxh"
                            android:layout_gravity="top|end"
                            android:layout_marginStart="@dimen/_10pxh"
                            android:layout_marginBottom="@dimen/_10pxv"
                            android:scaleType="fitXY"
                            android:src="@drawable/img_checkmark"/>
                    </FrameLayout>

                    <ImageView
                        android:id="@+id/imageIcwashroomOne"
                        android:layout_width="@dimen/_41pxh"
                        android:layout_height="@dimen/_39pxh"
                        android:layout_marginStart="@dimen/_37pxh"
                        android:layout_marginTop="@dimen/_16pxv"
                        android:scaleType="fitXY"
                        android:src="@drawable/img_icwashroom_15x15"/>

                    <ImageView
                        android:id="@+id/imageIcfoodOne"
                        android:layout_width="@dimen/_43pxh"
                        android:layout_height="@dimen/_41pxh"
                        android:layout_marginStart="@dimen/_43pxh"
                        android:layout_marginTop="@dimen/_10pxv"
                        android:layout_marginBottom="@dimen/_4pxv"
                        android:scaleType="fitXY"
                        android:src="@drawable/img_icfood_15x16"/>
                </LinearLayout>

                <LinearLayout
                    android:id="@+id/linearRowpharmacyOne"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:layout_marginStart="@dimen/_3pxh"
                    android:layout_marginTop="@dimen/_11pxv"
                    android:layout_marginEnd="@dimen/_10pxh"
                    android:gravity="start"
                    android:orientation="horizontal">

                    <TextView
                        android:id="@+id/txtPharmacyOne"
                        style="@style/txtPoppinsmedium12"
                        android:layout_width="0dp"
                        android:layout_height="wrap_content"
                        android:layout_weight="0.33"
                        android:ellipsize="end"
                        android:singleLine="true"
                        android:text="@string/lbl_pharmacy" />

                    <TextView
                        android:id="@+id/txtWiFi"
                        style="@style/txtPoppinsmedium12"
                        android:layout_width="0dp"
                        android:layout_height="wrap_content"
                        android:layout_marginStart="@dimen/_43pxh"
                        android:layout_weight="0.13"
                        android:ellipsize="end"
                        android:singleLine="true"
                        android:text="@string/lbl_wifi" />

                    <TextView
                        android:id="@+id/txtWashroomOne"
                        style="@style/txtPoppinsmedium12"
                        android:layout_width="0dp"
                        android:layout_height="wrap_content"
                        android:layout_marginStart="@dimen/_42pxh"
                        android:layout_weight="0.35"
                        android:ellipsize="end"
                        android:singleLine="true"
                        android:text="@string/lbl_washroom"/>

                    <TextView
                        android:id="@+id/txtFoodsOne"
                        style="@style/txtPoppinsmedium12"
                        android:layout_width="0dp"
                        android:layout_height="wrap_content"
                        android:layout_marginStart="@dimen/_34pxh"
                        android:layout_weight="0.19"
                        android:ellipsize="end"
                        android:singleLine="true"
                        android:text="@string/lbl_foods"/>
                </LinearLayout>
            </LinearLayout>
        </LinearLayout>
    </LinearLayout>
</LinearLayout>